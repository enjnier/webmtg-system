{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Docs: Serve MkDocs",
            "type": "debugpy",
            "request": "launch",
            "module": "mkdocs",
            "args": [
                "serve",
                "-a",
                "127.0.0.1:8100"
            ],
            "cwd": "${workspaceFolder}/docs",
            "console": "integratedTerminal"
        }
    ]
} 